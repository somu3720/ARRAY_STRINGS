class Solution {
    public int mostWordsFound(String[] sentences) {
        
        int max  = 0;
        for(int i = 0; i < sentences.length; i++)
        {
            max = Math.max(max,(sentences[i].split(" ")).length);
        }
        
        return max;
    }
}


// https://leetcode.com/problems/maximum-number-of-words-found-in-sentences/discuss/1662598/2ms-99-or-Easy-Explanation-or-Java
